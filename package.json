{
  "name": "graphql-subscription-sandbox",
  "version": "0.0.1",
  "description": "Sandbox of GraphQL subscription powered by Nest on AWS",
  "author": "namikingsoft@gmail.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && next build src/client",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -f ormconfig-ts -d src/migrations -n init",
    "migration:run": "npm run typeorm -- migration:run -f ormconfig-ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/client": "3.3.17",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/graphql": "7.10.6",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/typeorm": "7.1.5",
    "apollo-server-express": "2.24.0",
    "graphql": "15.5.0",
    "graphql-postgres-subscriptions": "1.0.5",
    "graphql-tools": "7.0.5",
    "next": "10.2.0",
    "pg": "8.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.6",
    "subscriptions-transport-ws": "0.9.18",
    "typeorm": "0.2.32"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript-graphql-request": "3.2.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.0",
    "@nestjs/testing": "7.6.15",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.36",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.4",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
